@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<div class="row">

    <div class="col-md-4 grid-margin stretch-card">
        @await Component.InvokeAsync("SumProductList")
    </div>

    <div class="col-md-8 grid-margin stretch-card">
        @await Component.InvokeAsync("TraderList")
    </div>

</div>

<div class="col-12">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Döviz</h4>
            <div class="row">
                @await Component.InvokeAsync("ChangeList")
                <div class="col-md-7">
                    <div id="audience-map" class="vector-map"></div>
                </div>
            </div>
        </div>
    </div>
</div>

